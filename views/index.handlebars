<div class="col-md-4">
	<ul>
		{{#each burgers}}
			{{#unless devoured}}
				<li>
					#{{id}}
					{{burger_name}}

					<form action="/{{id}}?_method=PUT" method="POST">
					  <input type="hidden" name="devoured" value="1">
					  <button type="submit">Devour?</button>
					</form>
				</li>
			{{/unless}}
		{{/each}}
	</ul>
</div>
<div class="col-md-4 col-md-offset-4 devoured">
	<ul>
		{{#each burgers}}
			{{#if devoured}}
	    		<li>
	    			#{{id}} 
	    			{{burger_name}}
	    		</li>
	    	{{/if}}
		{{/each}}
	</ul>
</div>